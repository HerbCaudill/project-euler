{
  "name": "project-euler",
  "version": "0.1.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "test": "tsdx test --verbose",
    "barrel": "barrelsby -c barrelsby.json",
    "postbarrel": "prettier --write \"**/index.ts\" --loglevel warn",
    "build": "yarn barrel",
    "next": "node next.js",
    "postnext": "yarn barrel"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "barrelsby": "^2.1.1",
    "prettier": "^1.17.1",
    "tsdx": "^0.5.9",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/ramda": "^0.26.8",
    "ramda": "^0.26.1"
  }
}
