{
  "name": "project-euler",
  "version": "0.1.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "test": "tsdx test --watch",
    "barrel": "barrelsby -c barrelsby.json",
    "postbarrel": "prettier --write \"**/index.ts\" --loglevel warn",
    "build": "yarn barrel",
    "next": "node next.js",
    "postnext": "yarn barrel"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "barrelsby": "^2.1.1",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tsdx": "^0.5.9",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "dependencies": {}
}
